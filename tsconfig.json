{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "$elements/*": ["components/elements/*"],
      "$blocks/*": ["components/blocks/*"],
      "$svgs/*": ["components/svgs/*"],
      "$layouts/*": ["components/layouts/*"],
      "$utils/*": ["utils/*"],
      "$stores/*": ["stores/*"],
      "$fixtures/*": ["fixtures/*"],
      "$types/*": ["types/*"],
      "$hooks/*": ["hooks/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "temp.js"],
  "exclude": ["node_modules"]
}
